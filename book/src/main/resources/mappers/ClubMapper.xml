<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.books.sys.menu.MenuMapper">
	
	<!-- 메뉴 목록 -->
	<select id="selectMenuList" parameterType="project.books.sys.menu.MenuVO" resultType="project.books.sys.util.CamelMap">
		/*MenuMapper.selectMenuList*/
		select * 
		  from menu m 
		 order by m.menu_no asc 
	</select>
</mapper>